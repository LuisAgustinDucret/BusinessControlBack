    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>f1b081e8-292c-4982-9c18-5b95edfaa73a</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="12.0.0" />
